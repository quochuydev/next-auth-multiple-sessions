datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

generator client {
    provider = "prisma-client-js"
}

model UserSession {
    id           String    @id @default(uuid())
    authSession  String
    userId       String?
    accessToken  String
    tokenType    String
    expiresIn    Int //miliseconds
    refreshToken String?
    idToken      String?
    createdAt    DateTime  @default(now())
    deletedAt    DateTime?
}
